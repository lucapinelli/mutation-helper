{
  "name": "mutation-helper",
  "version": "1.0.2",
  "description": "A collection of methods to mutate objects and arrays without modifying the source.",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src",
    "lint:watch": "esw -w --clear src",
    "audit:production": "npm audit --only=prod --audit-level=high"
  },
  "browserslist": "> 0.25%, not dead",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucapinelli/mutation-helper.git"
  },
  "keywords": [
    "mutate",
    "update",
    "immutability",
    "array",
    "object",
    "merge",
    "set",
    "get"
  ],
  "author": "Luca Pinelli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucapinelli/mutation-helper/issues"
  },
  "homepage": "https://github.com/lucapinelli/mutation-helper#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  }
}
